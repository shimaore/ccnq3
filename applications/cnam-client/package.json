  }
, "dependencies": {
    "ccnq3": ">=0.3.3"
  , "request": "~2.88.0"
  , "esl": ">=0.2.19 ~0.2"
  }
, "engines": { "node": ">=0.4.7", "coffee": ">=1.1.1" }
, "private": true
}
